{
  "name": "@yggdrasil-au/capacitor-assets",
  "version": "3.0.6",
  "description": "Generate icon and splash screen images for Capacitor apps",
  "homepage": "https://ionicframework.com",
  "author": "Ionic Team <hi@ionicframework.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.3.0"
  },
  "bin": {
    "capacitor-assets": "./bin/capacitor-assets"
  },
  "scripts": {
    "old::prepare": "husky install",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest --maxWorkers=4",
    "lint": "npm run eslint && npm run prettier -- --check",
    "fmt": "npm run prettier -- --write",
    "eslint": "eslint . --ext ts",
    "prettier": "prettier 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "prettier": "@ionic/prettier-config",
  "eslintConfig": {
    "extends": "@ionic/eslint-config/recommended"
  },
  "files": [
    "bin",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-team/capacitor-assets.git"
  },
  "bugs": {
    "url": "https://github.com/ionic-team/capacitor-assets/issues"
  },

  "dependencies": {
    "@ionic/utils-array": "2.1.6",
    "@ionic/utils-fs": "3.1.7",
    "@ionic/cli-framework-output": "2.2.8",
    "@ionic/utils-subprocess": "3.0.1",
    "@ionic/utils-terminal": "2.3.5",
    "kleur": "4.1.5",
    "@trapezedev/project": "7.1.3",
    "commander": "14.0.3",
    "debug": "4.4.3",
    "fs-extra": "11.3.3",
    "node-fetch": "3.3.2",
    "node-html-parser": "7.0.2",
    "sharp": "0.34.5",
    "tslib": "2.8.1",
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "8.0.2",
    "@changesets/cli": "2.29.8",
    "@ionic/eslint-config": "0.4.0",
    "@ionic/prettier-config": "4.0.0",
    "@types/debug": "4.1.12",
    "@types/fs-extra": "11.0.4",
    "@types/jest": "30.0.0",
    "@types/node": "25.1.0",
    "@types/node-fetch": "3.0.2",
    "@types/plist": "3.0.5",
    "@types/yargs": "17.0.35",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "np": "11.0.2",
    "prettier": "3.8.1",
    "rimraf": "6.1.2",
    "tempy": "3.1.2",
    "ts-jest": "29.4.6",
    "typescript": "5.9.3"
  },
  "overrides": {
    "@capacitor/cli": {
      "plist": {
        "@xmldom/xmldom": "0.8.4"
      }
    },
    "@trapezedev/project": {
      "@xmldom/xmldom": "0.8.11",
      "mergexml": {
        "@xmldom/xmldom": "0.7.7"
      }
    }
  }
}
